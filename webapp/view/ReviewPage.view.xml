<mvc:View controllerName="com.training.bootcampcss.controller.ReviewPage"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Page id="idReviewPage" title="{i18n>reviewTitle}" floatingFooter="true" class="header sapMBar-CTX sapMTitle" >
        <content>
            <core:Fragment fragmentName="com.training.bootcampcss.fragment.ShoppingCart" type="XML" class="header sapMBar-CTX sapMTitle" />
        </content>
        <footer>
            <OverflowToolbar id="idOFTFooterReview">
                <ToolbarSpacer id="idTSpacerFooterReview"/>
                <Button id="idBtnBack" text="{i18n>backBtn}" tooltip="{i18n>backTooltip}" press="onPressBack" class="backButton"/>
            </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>